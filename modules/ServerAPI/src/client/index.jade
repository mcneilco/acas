extends layout
block content
    div#appContainer



    script
        window.AppLaunchParams = !{JSON.stringify(AppLaunchParams)};
        window.addEventListener('load', function() {
            window.servicesHostPrefix = "";
            window.appController = new ModuleMenusController({
                el: $('#appContainer'),
                menuListJSON: (typeof ModuleMenusConfiguration !== "undefined") ? ModuleMenusConfiguration : null
            });
            window.appController.render();
        });